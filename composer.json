{
    "name": "work/excange",
    "description": "work exchange",
    "keywords": ["yii2", "framework"],
    "homepage": "http://work.com",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~2.0.5",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "kartik-v/yii2-widget-datepicker": "*",
        "kartik-v/yii2-field-range": "*",
        "elisdn/yii2-hybrid-authmanager": "^1.0",
        "yiisoft/yii2-jui": "^2.0",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "cetver/yii2-language-selector": "^1.0",
        "klisl/yii2-languages": "^2.8",
        "codemix/yii2-localeurls": "^1.7",
        "avator/yii2-turbosms": "*",

        "udokmeci/yii2-phone-validator" : "~1.0.3",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "kartik-v/yii2-mpdf": "dev-master",
        "kartik-v/yii2-widget-select2": "dev-master",
        "kartik-v/yii2-widget-rating": "*",
        "diecoding/yii2-dropify": "*",
        "yii2tech/html2pdf": "*",
        "yiisoft/yii2-imagine": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/codeception": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3"
    },
    "config": {
        "process-timeout": 1800,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
